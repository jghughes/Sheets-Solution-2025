    "@typescript-eslint/parser": "^8.0.0",
    "cpx": "^1.5.0",
    "esbuild": "^0.27.0",
    "eslint": "^9.0.0",
    "eslint-plugin-import": "^2.0.0",
    "jest": "^30.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  }
