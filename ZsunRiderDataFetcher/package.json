    "copy-html": "cpx \"src/**/*.html\" dist",
    "copy-manifest": "cpx \"appsscript.json\" dist",
    "push": "npx clasp push",
    "deploy": "npm run build && npm run push",
    "test": "jest",
    "lint": "eslint ."
  },
